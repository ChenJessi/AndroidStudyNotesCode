<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--    <external-files-path path="/apk" name="apk"/>-->

    <!--代表外部存储区域的根目录下的文件 Environment.getExternalStorageDirectory()/DCIM/XX目录-->
    <external-path name="cc_DCIM" path="DCIM/XX" />
    <!--代表外部存储区域的根目录下的文件 Environment.getExternalStorageDirectory()/Pictures/XX目录-->
    <external-path name="cc_Pictures" path="Pictures/XX" />
    <!--代表app 私有的存储区域 Context.getFilesDir()目录下的images目录 /data/user/0/com.chencc.androidstudynotescode/files/images-->
    <files-path name="cc_private_files" path="images" />
    <!--代表app 私有的存储区域 Context.getCacheDir()目录下的images目录 /data/user/0/com.chencc.androidstudynotescode/cache/images-->
    <cache-path name="cc_private_cache" path="images" />
    <!--代表app 外部存储区域根目录下的文件 Context.getExternalFilesDir(Environment.DIRECTORY_PICTURES)目录下的 Pictures目录-->
    <!--/storage/emulated/0/Android/data/com.chencc.androidstudynotescode/files/Pictures-->
    <external-files-path name="cc_external_files" path="Pictures" />
    <!--代表app 外部存储区域根目录下的文件 Context.getExternalCacheDir目录下的images目录-->
    <!--/storage/emulated/0/Android/data/com.chencc.androidstudynotescode/cache/images-->
    <external-cache-path name="cc_external_cache" path="images" />

    <!--/storage/emulated/0/app/下的目录 内部存储-->
    <external-path name="external-path" path="app/" />
</resources>